{
  "name": "riseway",
  "description": "ðŸš€ Fast, safe and modern",
  "keywords": [
    "nodejs",
    "fast",
    "performance",
    "secure"
  ],
  "version": "0.0.0",
  "exports": {
    ".": {
      "default": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./middleware": {
      "default": {
        "import": "./dist/middleware.js",
        "types": "./dist/middleware.d.ts"
      },
      "import": "./dist/middleware.js",
      "types": "./dist/middleware.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.build.json --outDir dist",
    "postbuild": "tsc-alias -f -p tsconfig.build.json --outDir dist && tsc -p tsconfig.build.json --emitDeclarationOnly --declaration --outDir dist",
    "lint": "eslint . --cache --concurrency auto",
    "test": "jest --passWithNoTests",
    "test:ci": "jest --passWithNoTests --ci",
    "test:watch": "jest --passWithNoTests --watch",
    "test:watchAll": "jest --passWithNoTests --watchAll",
    "test:coverage": "jest --passWithNoTests --coverage",
    "benchmark": "k6 run tools/benchmark.js",
    "prepare": "husky"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.0",
    "@jest/globals": "^30.2.0",
    "@swc/jest": "^0.2.39",
    "@types/node": "^24.10.1",
    "eslint": "^9.39.1",
    "eslint-plugin-jest": "^29.2.1",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  },
  "author": {
    "name": "Luciano Alves",
    "url": "https://github.com/luas10c"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/luas10c/riseway"
  },
  "bugs": {
    "url": "https://github.com/luas10c/riseway/issues"
  },
  "engines": {
    "node": "^20.x || ^22.x || ^24.x"
  },
  "license": "MIT"
}
